        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "ejs": "^2.6.1",
        "express": "^4.17.1",
        "express-session": "^1.16.2",
        "express-validator": "^3.2.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.6.0",
        "multer": "^1.4.1",
