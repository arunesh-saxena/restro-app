{
    "name": "restro-app-server",
    "version": "1.0.0",
    "description": "server api for restro app",
    "main": "app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev2": "nodemon index.js",
        "start": "node ./compiled/index.js",
        "build": "webpack --config ./webpack/webpack.server.js --mode production",
