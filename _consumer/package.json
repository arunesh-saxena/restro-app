{
    "name": "consumer-app",
    "version": "1.0.0",
    "description": "consumer app for placeing order",
    "main": "index.js",
    "scripts": {
        "start:spa": "webpack-dev-server --config ./webpack/webpack.config.js --mode development --open",
        "start": "node ./compiled/index.js",
        "build": "npm run clean && npm run copy && webpack --config ./webpack/webpack.server.client.js --mode production && webpack --config ./webpack/webpack.server.js --mode production",
        "dev:build-client": "webpack --config ./webpack/webpack.server.client.js --mode development -w",
        "dev:build-server": "NODE_ENV=development webpack --config ./webpack/webpack.server.js --mode development -w",
        "dev:start": "nodemon ./compiled/index.js",
        "dev": "npm run copy && npm-run-all --parallel dev:build-client dev:build-server dev:start",
        "setup": "npm run clean && npm run copy",
        "clean": "rimraf public && rimraf compiled",
        "copy": "copyfiles -f ./app/fonts/*.* ./public/assets/fonts && copyfiles -f ./app/images/*.* ./public/assets/images",
        "lint": "npm run lint:js && npm run lint:scss",
        "lint:js": "eslint --quiet --ext .js,.jsx app server",
        "lint:scss": "stylelint --quiet \"**/*.scss\"",
        "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
        "lint:js:fix": "eslint --quiet --ext .js,.jsx app server --fix",
        "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
        "pretty": "prettier --write '{app,server}/**/*.{js,jsx,json,scss}'",
        "format": "prettier-eslint  --write '{app,server}/**/*.{js,jsx,json,scss}'"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && lint-staged",
            "pre-push": "echo pushing....."
        }
    },
    "lint-staged": {
        "*.{js,jsx,json,scss}": [
            "prettier-eslint --write",
            "git add"
        ]
    },
    "keywords": [
        "restro",
        "app"
    ],
    "author": "Arunesh Saxena",
    "license": "ISC",
    "engines": {
        "node": ">=10.10.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.41",
        "@babel/core": "^7.0.0-beta.41",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "autoprefixer": "^8.6.4",
        "babel": "^6.23.0",
        "babel-core": "^7.0.0-beta.3",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^7.1.4",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-preset-env": "^7.0.0-beta.3",
        "babel-preset-react": "^7.0.0-beta.3",
        "clean-webpack-plugin": "^0.1.19",
        "copyfiles": "^2.1.0",
        "css-loader": "^0.28.11",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.13.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^1.1.11",
        "form-data": "^2.3.3",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^2.3.0",
        "lint-staged": "^8.1.7",
        "mini-css-extract-plugin": "^0.7.0",
        "node-sass": "^4.11.0",
        "nodemon": "^1.18.7",
        "npm-run-all": "^4.1.5",
        "postcss-loader": "^2.1.5",
        "prettier": "^1.17.1",
        "prettier-eslint": "^8.8.2",
        "prettier-eslint-cli": "^4.7.1",
        "sass-loader": "^7.0.3",
        "style-loader": "^0.21.0",
        "stylelint": "^10.0.1",
        "stylelint-config-standard": "^18.3.0",
        "url-loader": "^1.1.2",
        "webpack": "^4.26.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.14",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "bootstrap": "^4.1.3",
        "cookie-parser": "^1.4.4",
        "express": "^4.16.4",
        "lodash": "^4.17.10",
        "multer": "^1.4.1",
        "node-fetch": "^2.3.0",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-loadable": "^5.5.0",
        "react-redux": "^6.0.0",
        "react-router": "^4.3.1",
        "react-router-config": "^4.4.0-beta.6",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.1",
        "redux-form": "^8.1.0",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0"
    }
}
